{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "ES2022",
    "esModuleInterop": true,
    "allowJs": false,
    "strict": true,
    "jsx": "react",
    "outDir": "buildServer",
    "moduleResolution": "nodenext"
  },
  "include": ["server/**/*.ts"]
}
